
import java.io.*;

class Arithmetic 
{
    public static void main (String[] args) 
    {
        
        int a = 10;
        int b = 3;
    
        String n1 = "15";
        String n2 = "25";

        int a1 = Integer.parseInt(n1);
        int b1 = Integer.parseInt(n2);
           
        System.out.println("a + b = " + (a + b));
        System.out.println("a - b = " + (a - b));
        System.out.println("a * b = " + (a * b));
        System.out.println("a / b = " + (a / b));
        System.out.println("a % b = " + (a % b));
        System.out.println("a1 + b1 = " + (a1 + b1)); 
          
    }
}
